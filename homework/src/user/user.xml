<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="use">
	<select id="getUser" resultType="userDataBean">
		select * from userU where user_id=#{user_id}
	</select>
	<insert id="intoUser" parameterType="userDataBean">
		insert into userU(user_id, password, user_name, email, reg_date, gender)
			value(#{user_id}, #{password}, #{user_name}, #{email}, #{reg_date}, #{gender})
	</insert>
	<update id="updateUser" parameterType="userDataBean">
		0
	</update>
	<delete id="deleteUser" parameterType="String">
		delete from userU where user_id=#{user_id}
	</delete>



</mapper>